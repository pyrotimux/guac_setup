<user-mapping>
{% for pusr in users %}
  <authorize username="{{ pusr.name }}" password="{{ pusr.pass }}">
{% for psrv in server_dict %}
    <connection name="{{ psrv.server }}">
      <protocol>rdp</protocol>
      <param name="hostname">{{ psrv.ip }}</param>
      <param name="port">{{ window_port }}</param>
    </connection>
{% endfor %}
  </authorize>
{% endfor %}
</user-mapping>
